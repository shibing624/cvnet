{
    "dataset": "default",
    "task": "det",
    "method": "faster_rcnn",
    "data": {
      "num_classes": 21,
      "data_dir": "~/DataSet/VOC07_DET",
      "image_tool": "cv2",
      "input_mode": "BGR",
      "keep_difficult": false,
      "workers": 8,
      "mean_value": [123, 116, 103],
      "normalize": {
        "div_value": 1.0,
        "mean": [122.7717, 115.9465, 102.9801],
        "std": [1.0, 1.0, 1.0]
      }
    },
    "train": {
      "batch_size": 1,
      "aug_trans": {
        "trans_seq": ["random_hflip", "random_resize"],
        "random_hflip": {
          "ratio": 0.5,
          "swap_pair": []
        },
        "random_resize": {
          "ratio": 1.0,
          "method": "bound",
          "resize_bound": [600, 1000],
          "aspect_range": [0.9, 1.1]
        }
      },
      "data_transformer": {
        "samples_per_gpu": 1,
        "size_mode": "max_size",
        "fit_stride": 16,
        "align_method": "only_pad",
        "pad_mode": "pad_right_down"
      }
    },
    "val": {
      "batch_size": 1,
      "use_07_metric": true,
      "aug_trans": {
        "trans_seq": ["random_resize"],
        "random_resize": {
          "ratio": 1.0,
          "method": "bound",
          "resize_bound": [600, 1000],
          "aspect_range": [1.0, 1.0]
        }
      },
      "data_transformer": {
        "samples_per_gpu": 1,
        "size_mode": "max_size",
        "fit_stride": 16,
        "align_method": "only_pad",
        "pad_mode": "pad_right_down"
      }
    },
    "test": {
      "batch_size": 4,
      "aug_trans": {
        "trans_seq": ["resize"],
        "resize": {
          "min_side_length": 600,
          "max_side_length": 1000
        },
      },
      "data_transformer": {
        "samples_per_gpu": 1,
        "size_mode": "max_size",
        "fit_stride": 16,
        "align_method": "only_pad",
        "pad_mode": "pad_right_down"
      }
    },
    "rpn": {
      "head_index_list": [0],
      "anchor_sizes_list": [
        [[128, 128], [256, 256], [512, 512]]
      ],
      "num_anchor_list": [9],
      "aspect_ratio_list": [[2]],
      "stride_list": [16],
      "num_feature_list": [512],
      "nms_threshold": 0.7,
      "min_size": 16,
      "sampler": {
        "n_sample": 256,
        "pos_ratio": 0.5,
        "pos_iou_thresh": 0.7,
        "neg_iou_thresh": 0.3
      },
      "n_train_pre_nms": 12000,
      "n_train_post_nms": 2000,
      "n_test_pre_nms": 6000,
      "n_test_post_nms": 300
    },
    "roi": {
      "method": "roipool",
      "pooled_hw": [7, 7],
      "spatial_stride": 16,
      "loc_normalize_mean": [0.0, 0.0, 0.0, 0.0],
      "loc_normalize_std": [0.1, 0.1, 0.2, 0.2],
      "sampler": {
        "n_sample": 128,
        "pos_ratio": 0.25,
        "pos_iou_thresh": 0.5,
        "neg_iou_thresh_hi": 0.5,
        "neg_iou_thresh_lo": 0.0
      }
    },
    "details": {
      "color_list": [[255, 170, 30], [0, 0, 70], [244, 35, 232]],
      "name_id_dict": {
        "aeroplane": 1, "bicycle": 2, "bird": 3, "boat": 4, "bottle": 5, "bus": 6, "car": 7,
        "cat": 8, "chair": 9, "cow": 10, "diningtable": 11, "dog": 12, "horse": 13, "motorbike": 14,
        "person": 15, "pottedplant": 16, "sheep": 17, "sofa": 18, "train": 19, "tvmonitor": 20
      },
      "name_seq": ["aeroplane", "bicycle", "bird", "boat", "bottle", "bus", "car", "cat", "chair",
                   "cow", "diningtable", "dog", "horse", "motorbike", "person", "pottedplant",
                   "sheep", "sofa", "train", "tvmonitor"]
    },
    "network":{
      "backbone": "vgg16",
      "model_name": "faster_rcnn",
      "use_drop": false,
      "checkpoints_name": "fr_vgg16_voc_det",
      "checkpoints_dir": "checkpoints/det/voc"
    },
    "solver": {
      "lr": {
        "metric": "epoch",
        "base_lr": 0.001,
        "lr_policy": "step",
        "step": {
          "gamma": 0.1,
          "step_size": 9
        }
      },
      "optim": {
        "optim_method": "sgd",
        "sgd":{
          "weight_decay": 0.0005,
          "momentum": 0.9,
          "nesterov": false
        }
      },
      "display_iter": 100,
      "save_iters": 5000,
      "test_interval": 5000,
      "max_epoch": 15
    },
    "res": {
      "nms": {
        "mode": "union",
        "max_threshold": 0.45
      },
      "val_conf_thre": 0.05,
      "vis_conf_thre": 0.5
    },
    "loss": {
      "loss_type": "fasterrcnn_loss",
      "loss_weights": {
        "fasterrcnn_loss": {
            "rpn_loc_loss": 1.0, "rpn_cls_loss": 1.0, "roi_loc_loss": 1.0, "roi_cls_loss": 1.0
        },
      },
      "params": {
        "rpn_sigma": 3.0,
        "roi_sigma": 1.0,
        "ce_loss": {
            "reduction": "mean",
            "ignore_index": -1
        }
      }
    }
}
